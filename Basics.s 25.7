for loop 3.1
#print prime number from m to n
#include <iostream>

using namespace std;

int main() 
{
    int n,m;
    cin>>n>>m;
    for(int num=n;num<=m;num++){
        int i;
        for( i=2;i<=num;i++){
            if(num%i==0){
                break;
            }
        }
        if(i==num){
            cout<<i<<" ";
        }
    }
    
    return 0;
}
